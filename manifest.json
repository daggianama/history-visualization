{
  "manifest_version": 3,
  "name": "history visualizer",
  "version": "1.0",
  "description": "Understand your web interactions data with graphics",

  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "permissions": [
    "history"
  ],
  "action": {
    "default_popup": "history-visualizer.html",
    "default_icon": "chart-simple-solid.svg"
  }
}


